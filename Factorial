//Topic- To find the faqctorial of given number 
#include<iostream>
#include<math.h>
using namespace std;
int prime(int);

int main()
{
	bool isprime;
	int p;
	cout<<"enter the limit"<<endl;
	cin>>p;
	
	for(int n=2; n<p; n++)
	{isprime = prime(n);
	
	if(isprime == true )
	cout<<n<<endl;
	}
	cout <<"execute by Deep Akash";
	return 0;
	
}
int prime (int n)
{
bool isprime = true;

for(int i=2; i<=sqrt(n);i++)
	{

if(n%i==0)
{isprime = false;
break;
}
 
return isprime;
}}
